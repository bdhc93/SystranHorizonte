<table class="table table-hover table-striped">
    <thead>
        <tr>
            <th>TarjetaPropiedad</th>
            <th>NroPlaca</th>
            <th>FechaSoat</th>
            <th>FechaRevisionTecnica</th>
            <th>Ancho</th>
            <th>Largo</th>
            <th>Asientos</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.TarjetaPropiedad</td>
                <td>@item.NroPlaca</td>
                <td>@item.FechaSoat</td>
                <td>@item.FechaRevisionTecnica</td>
                <td>@item.Ancho</td>
                <td>@item.Largo</td>
                <td>@item.Asientos</td>
                <td><a href="@Url.Action("Modificar", "Vehiculo", new { id = @item.Id })">Editar</a> | <a href="#" onclick="abrirmodalelim(@item.Id)">Eliminar</a></td>
            </tr>
        }
    </tbody>
</table>
