<table class="table table-hover table-striped">
    <thead>
        <tr>
            <th>Fecha</th>
            <th>Hora</th>
            <th>PlacaVehiculo</th>
            <th>TarjetaVehiculo</th>
            <th>EstacionOrigen</th>
            <th>EstacionDestino</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Fecha</td>
                <td>@item.Hora</td>
                <td>@item.NroPlaca</td>
                <td>@item.TarjetaPropiedad</td>
                <td>@item.OrigenId</td>
                <td>@item.DestinoId</td>
            </tr>
        }
    </tbody>
</table>